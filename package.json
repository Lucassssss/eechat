{
  "name": "eechat",
  "version": "0.7.5",
  "main": "packages/electron-main/dist/index.js",
  "description": "a light weight chat tool for llm.",
  "author": "eechat <oodoobvideo@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lucassssss/eechat.git"
  },
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "tsc -w -p packages/electron-main/tsconfig.json && electron packages/electron-main/dist/index.js",
    "build": "tsc -p packages/electron-main/tsconfig.json",
    "start": "pnpm run build && electron packages/electron-main/dist/index.js",
    "dev:server": "cd electron/server && egg-bin dev",
    "dev:packages": "pnpm run -r dev",
    "build:packages": "pnpm run -r build",
    "clean:packages": "pnpm run -r clean",
    "build:win": "vue-tsc --noEmit && vite build && electron-builder --publish always --win",
    "package": "vue-tsc --noEmit && vite build && electron-builder",
    "build:mac": "vue-tsc --noEmit && vite build && electron-builder --publish always --mac",
    "build:pord": "vite build --mode production && electron-builder",
    "r:server": "node scripts/release-server.mjs",
    "r:github": "node scripts/release-github.mjs",
    "preview": "vite preview",
    "release": "node scripts/release.js",
    "i18n:extract": "vue-i18n-extract report --vueFiles \"src/**/*.?(js|vue)\" --languageFiles \"src/locales/zh/index.json\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "tsup": "^8.4.0",
    "turbo": "^2.5.2",
    "typescript": "^5.4.2"
  },
  "packageManager": "pnpm@9.14.2+sha512.6e2baf77d06b9362294152c851c4f278ede37ab1eba3a55fda317a4a17b209f4dbb973fb250a77abc463a341fcb1f17f17cfa24091c4eb319cda0d9b84278387"
}
